{
  "name": "games",
  "version": "1.1.1",
  "description": "A collection of simple games. ",
  "license": "MIT",
  "repository": "github:/sp1tz/games.git",
  "bugs": "https://github.com/sp1tz/games/issues",
  "author": "sp1tz (https://sp1tz.github.io)",
  "dependencies": {
    "http-server": "^14.1.1"
  },
  "devDependencies": {
    "jshint": "^2.13.6"
  },
  "scripts": {
    "start": "http-server app/",
    "test": "jshint app/"
  },
  "jshintConfig": {
    "bitwise": true,
    "browser": true,
    "curly": true,
    "eqeqeq": true,
    "esversion": 11,
    "expr": true,
    "forin": true,
    "freeze": true,
    "futurehostile": true,
    "latedef": "nofunc",
    "leanswitch": true,
    "noarg": true,
    "nocomma": true,
    "node": true,
    "nonbsp": true,
    "nonew": true,
    "noreturnawait": true,
    "singleGroups": true,
    "strict": "global",
    "trailingcomma": true,
    "undef": true,
    "unused": true,
    "validthis": true,
    "varstmt": true
  }
}
